{
    "name": "eaglexboy/ebay-oauth-php-client",
    "type": "library",
    "description": "eBay OAuth Client which helps in easy integration",
    "keywords": ["ebay", "oauth", "client"],
    "license": "Apache-2.0",
    "require": {
        "monolog/monolog": "^3.7",
        "php": ">=8.1",
        "symfony/http-client": "^7.1",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "Ebay\\Api\\Client\\Auth\\OAuth2\\": "src/main/"
        },
        "exclude-from-classmap": [
            "**/tests/*",
            "**/bin/*"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ebay\\Api\\Client\\Auth\\OAuth2\\Tests\\": "src/tests/"
        }
    }
}
